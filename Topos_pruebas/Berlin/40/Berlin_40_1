set topo [new Topography]
$topo load_flatgrid 615.8056827446969 499.7668110133443
create-god 40 
$ns_ node-config -adhocRouting $val(rp) -llType $val(ll) -macType $val(mac)  -ifqType $val(ifq) -ifqLen $val(ifqlen) -antType $val(ant) -propType $val(prop) -phyType $val(netif) -channel  [new $val(chan)] -topoInstance $topo -agentTrace ON -routerTrace OFF -macTrace OFF -movementTrace OFF 
# maximum node distance allowed (if distance is larger, it is assumed that link does not exist) 90.0
for {set i 0} {$i < 40} {incr i} { set node_($i) [$ns_ node] } 
